# Environment Variables for Visita Application
# Copy this file to .env and fill in your values

# Database Configuration (JawsDB MySQL on Heroku)
JAWSDB_URL=jdbc:mysql://your-host:3306/your-database
DB_USERNAME=your_username
DB_PASSWORD=your_password

# JWT Secret (generate a strong random string)
JWT_SECRET=your_jwt_secret_here

# MoMo Payment Configuration
MOMO_PARTNER_CODE=your_partner_code
MOMO_ACCESS_KEY=your_access_key
MOMO_SECRET_KEY=your_secret_key
MOMO_ENDPOINT=https://payment.momo.vn/v2/gateway/api/create
MOMO_REDIRECT_URL=https://your-domain.com/payment-result
MOMO_IPN_URL=https://your-domain.com/api/payment/ipn-momo

# PayPal Payment Configuration
PAYPAL_MODE=live
PAYPAL_CLIENT_ID=your_paypal_client_id
PAYPAL_CLIENT_SECRET=your_paypal_client_secret
PAYPAL_BASE_URL=https://api-m.paypal.com
